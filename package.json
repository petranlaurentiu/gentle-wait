{
  "name": "gentle-wait",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_OPTIONS='--no-warnings' expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "NODE_OPTIONS='--no-warnings' expo run:android",
    "ios": "NODE_OPTIONS='--no-warnings' expo run:ios",
    "web": "NODE_OPTIONS='--no-warnings' expo start --web",
    "lint": "expo lint",
    "setup-ios": "PATH=\"/opt/homebrew/lib/ruby/gems/3.4.0/bin:$PATH\" ./scripts/setup-ios-native.sh",
    "prebuild:ios": "PATH=\"/opt/homebrew/lib/ruby/gems/3.4.0/bin:$PATH\" npx expo prebuild --platform ios --clean",
    "prebuild:android": "npx expo prebuild --platform android --clean",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:all": "eas build --profile production --platform all"
  },
  "dependencies": {
    "@expo-google-fonts/outfit": "^0.4.3",
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.31",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "^16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-device-activity": "^0.5.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-mmkv": "^4.1.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
